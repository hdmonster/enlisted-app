<div class="container">
    <% allPengurus.forEach(pengurus => { %>
        <div class="card-classic" onclick="navigateReplace('/mahasiswa/id/<%= pengurus.id  %>/view')">
            <img src="<%= pengurus.photoURL %>" alt="Profile Pic">
            <span class="title"><%= pengurus.fullName %></span>
        </div>
    <% }) %>
</div>
<!-- Toast -->
<script type="text/javascript">
    if('<%= messages.success %>'){
        showToast('success','<%= messages.success %>')
    }
    if('<%= messages.err %>'){
        showToast('failed','<%= messages.err %>')
    }
</script>
