
<div class="container">
    <div class="recent_wrapper">
        <span>recent</span>
    </div>
    <% lists.forEach(function(list){ %>

        <div class="card-list" onclick="navigateNext('<%= list.id %>/view')">
            <div class="header">
                <span class="title"><%= list.title %></span>
                <span class="detail"><%= list.author.name.split('AKA ')[1] %> -  <%= moment(list.createdAt, "DD/MM/YYYY HH:mm:ss").fromNow() %></span>
            </div>
            <div class="icon-container">
                <i data-feather="chevron-right"></i>
            </div>
        </div>
    <% }) %>
</div>

<script src="/js/socket/list.js"></script>
